<?xml version="1.0"?>
<launch>

<!-- DONT FORGET TO
	* rosparam set use_sim_time true
	* change the path to the files

 => you *.csv file will be found in the ~/.ros directory-->

<!-- Setup -->
	<include file="$(find rawseeds_ros)/launch/transforms.launch" />
	


	<node pkg="map_server" type="map_server" name="map_server" args="/robotlog/rawseeds/Bicocca_2009_02_25a_acml_benchmark_extra_data/vector_map_bicocca_complete.yaml"/>

	<node pkg="amcl" type="amcl" name="amcl" args="/scan:=/sick_front"  output="screen"/>
	<node pkg="rawseeds_ros" name="publish_initial_pose" type="publish_initial_pose.sh" output="screen" />

	<node pkg="rosbag" type="play" name="player" output="screen" args="/robotlog/rawseeds/Bicocca_2009_02_25a_acml_benchmark_extra_data/Bicocca_2009-02-25b_odom_sick_front_gt.bag --clock  -r 2"/>



	<node pkg="rawseeds_ros" type="log_trajectory.py" name="log_trajectory" args=" --filename amcl_trajectory.csv --fixed_frame /map --moving_frame /base_link" output="screen" />

</launch>
